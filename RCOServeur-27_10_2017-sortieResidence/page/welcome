<script>
	isInit=false;
function initSound(){
	if( /Android|webOS|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
		screenfull.request();
	//noSleep.enable();
	
}

  	if(!isInit){
  	isInit=true;
     
         		
                o.type = "sine"
                o.connect(context.destination)
                o.start();
                isPlaying=true;
               	o.disconnect(context.destination)
                o.stop();
                isPlaying=false;  

  		
     		page('w-end');   
  		}
  		  	

  }
var p = document.getElementById("display");
  p.onclick = initSound;

</script>

